local redzlib=loadstring(game:HttpGet("https://raw.githubusercontent.com/tbao143/Library-ui/refs/heads/main/Redzhubui"))()local Window=redzlib:MakeWindow({Title="Tropa do Egito Hub",SubTitle="by Dkzinn",SaveFolder="Tropa_do_Egito_Hub_v1.lua"})Window:AddMinimizeButton({Button={Image="rbxassetid://71014873973869",BackgroundTransparency=0},Corner={CornerRadius=UDim.new(35,1)}})local Carros101=Window:MakeTab({"Chuva de Veículos","car"})local Section=Carros101:AddSection({"VERSÃO BETA"})local Section=Carros101:AddSection({"By; Dkzinn"})local Quantidade=20 local chuvaAtiva=false local remote=game:GetService("ReplicatedStorage"):WaitForChild("RE"):WaitForChild("1Ca1r")local carListV5={"Semi","Easter2025EasterMonsterTruck"}local Toggle1=Carros101:AddToggle({Name="Ativar Chuva de Veículos",Description="Ative ou desative a chuva de veículos no jogo",Default=false})Toggle1:Callback(function(v)chuvaAtiva=v if v then task.spawn(function()local plr=game.Players.LocalPlayer local char=plr.Character or plr.CharacterAdded:Wait()local root=char:WaitForChild("HumanoidRootPart")while chuvaAtiva do local posAtual=root.Position for i=1,Quantidade do if not chuvaAtiva then break end local alt=math.random(2500,5000)local spawnPos=CFrame.new(posAtual.X,alt,posAtual.Z)root.CFrame=spawnPos task.wait(0.05)for _,car in ipairs(carListV5)do remote:FireServer("PickingCar",car)task.wait(0.03)end root.CFrame=CFrame.new(posAtual)end task.wait(0.1)end end)else print("Chuva de veículos desativada!")end end)
